# omz
alias zshconfig="geany ~/.zshrc"
alias ohmyzsh="thunar ~/.oh-my-zsh"

# ls
alias l='ls -lh'
alias ll='ls -lah'
alias la='ls -A'
alias lm='ls -m'
alias lr='ls -R'
alias lg='ls -l --group-directories-first'

# git
alias gcl='git clone'
alias gi='git init'
alias ga='git add'
alias gaa="git add -A"
alias gcm='git commit -m'
alias gc='git commit'
alias gps='git push'
alias gpl='git pull'
alias gplr='git pull --rebase'

